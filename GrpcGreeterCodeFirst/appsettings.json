{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "AllowedHosts": "*",

  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2" // http1 is needed for .NET 4.x clients using Grpc-Web (see https://github.com/grpc/grpc-dotnet/issues/1552)
    }
  }
}
